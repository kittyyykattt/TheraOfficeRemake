<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Maui.TheraOffice.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Maui.WordPress"
    xmlns:view="clr-namespace:Maui.WordPress.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="Maui.TheraOffice">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="BlogDetail"
        ContentTemplate="{DataTemplate view:BlogView}"
        Route="Blog" />
</Shell>
